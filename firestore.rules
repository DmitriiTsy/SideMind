rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /botsList/{type} {
      allow read, write: if request.auth != null;
    }
    match /usersList/{userId} {
      allow read, write: if request.auth != null
    }
    match /prompts/{type} {
    	allow read, write: if request.auth != null
    }
    match /customBotsList/{type} {
    	allow read, write: if request.auth != null
    }
  }
}